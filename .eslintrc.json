{
    "env": {
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "no-undef": 0,
        "no-console": "off",
        "no-prototype-builtins": "off",
        "indent": ["error", 4],
        "semi": "error",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "no-unused-vars": 1
    }
}
